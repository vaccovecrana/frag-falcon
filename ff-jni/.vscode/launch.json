{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug with GDB",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/out/fg_test",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
